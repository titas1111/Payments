@model IEnumerable<Payment>

    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">MonthId</th>
                <th scope="col">Description</th>
                <th scope="col">Price</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.MonthId</td>
                    <td>@item.Description</td>
                    <td>@item.Price</td>
                </tr>
            }

        </tbody>
    </table>